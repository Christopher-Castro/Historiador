{
  "name": "historiador-web",
  "version": "1.0.0",
  "description": "A simple IoT Platform - Web Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prestart": "npm run build",
    "build": "NODE_ENV=production browserify -g envify -t babelify -t vueify client/app.js | uglifyjs -c -m -o public/bundle.js",
    "start-dev": "DEBUG=historiador:* nodemon server.js",
    "prestart-dev": "npm run build-dev",
    "build-dev": "browserify -g envify -t babelify -t vueify client/app.js -o public/bundle.js",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Christopher-Castro/Historiador.git"
  },
  "author": "Christopher Alexis Castro Garc√©s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Christopher-Castro/Historiador/issues"
  },
  "homepage": "https://github.com/Christopher-Castro/Historiador#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^17.0.0",
    "envify": "^4.1.0",
    "nodemon": "^2.0.6",
    "standard": "^16.0.3",
    "uglify-js": "github:mishoo/UglifyJS2#harmony",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "asyncify": "^2.1.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chart.js": "^2.9.4",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-asyncify": "^1.0.1",
    "historiador-agent": "file:../historiador-agent",
    "moment": "^2.29.1",
    "node-sass": "^7.0.1",
    "pg": "^8.7.3",
    "random-material-color": "^1.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.5.1"
  }
